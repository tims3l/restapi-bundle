{
  "name": "tims3l/restapi-bundle",
  "type": "symfony-bundle",
  "version": "0.1",
  "description": "tims3l restapi api symfony bundle",
  "keywords": [
    "tims3l",
    "restapi",
    "api",
    "symfony",
    "bundle"
  ],
  "homepage": "https://github.com/tims3l/restapi-bundle",
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-openssl": "*",
    "ext-mbstring": "*",
    "psr/log": "^3.0",
    "symfony/http-kernel": "^6.1",
    "spatie/url": "^2.1",
    "doctrine/orm": "^2.13",
    "symfony/validator": "^6.1",
    "symfony/orm-pack": "^2.2",
    "symfony/property-info": "^6.1"
  },
  "autoload": {
    "psr-4": {
      "Tims3l\\RestApi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tims3l\\RestApi\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "symfony/test-pack": "^1.0"
  }
}
